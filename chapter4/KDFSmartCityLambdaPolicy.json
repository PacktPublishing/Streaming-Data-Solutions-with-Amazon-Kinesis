{
	"Version": "2012-10-17",
	"Statement": [{
		"Effect": "Allow",
		"Action": "logs:CreateLogGroup",
		"Resource": "arn:aws:logs:us-east-1:862656071619:*"
	}, {
		"Effect": "Allow",
		"Action": ["logs:CreateLogStream", "logs:PutLogEvents"],
		"Resource": ["arn:aws:logs:us-east-1:862656071619:log-group:/aws/lambda/KDFLookupAddressTransform:*"]
	}, {
		"Effect": "Allow",
		"Action": "dynamodb:Scan",
		"Resource": "arn:aws:dynamodb:us-east-1:862656071619:table/BikeStationAddress"
	}]
}